$TTL 2d    ; default TTL for zone
$ORIGIN caspersbox.com. ; base domain-name
@           IN      SOA   ns1.caspersbox.com. dnsadm.ns1.caspersbox.com. (
            2025032101     ; serial number
            12h            ; refresh
            15m            ; update retry
            3w             ; expiry
            2h             ; minimum
            )
            IN      NS      ns1.caspersbox.com.

ns1                     IN      A       192.168.0.103
fedora-nfsserver        IN      A       192.168.10.5
fedora-dmserver         IN      A       192.168.10.6
fedora-appserver        IN      A       192.168.10.7
fedora-portalserver     IN      A       192.168.10.8
fedora-webserver        IN      A       192.168.10.9

